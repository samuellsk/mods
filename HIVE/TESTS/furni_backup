"ABYSSUS_HIVE_FLOOR_CANCER_TEST_2"
{
  name = "floor"
  viewId = { "abyssus_hive_floor_1" }
  luxury = 0.5
  layer = FLOOR
  canBuildOutsideOfTerritory = true
  tickType = Effect {
          FirstSuccessful {
    Filter Or {FurnitureType "MOUNTAIN" FurnitureType "MOUNTAIN2" FurnitureType "SANDSTONE" FurnitureType "GLACIER" FurnitureType "HELL_MOUNTAIN"} {
      PlaceFurniture "ABYSSUS_HIVE_WALL"
    }
    Filter Not FurnitureType "ABYSSUS_HIVE_FLOOR_CANCER_TEST_2_RANDOM" {
      Filter FurnitureType "FLOOR" {
        Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
          Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
            PlaceFurniture "ABYSSUS_HIVE_FLOOR_CANCER_TEST_2_RANDOM"
          }
        }
      }
    }
    Chance 0.30 {
      Area 1 Filter Not FurnitureType "ABYSSUS_HIVE_FLOOR_CANCER_TEST_2_RANDOM" {
        Filter FurnitureType "FLOOR" {
          Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
            Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
              PlaceFurniture "ABYSSUS_HIVE_FLOOR_CANCER_TEST_2_RANDOM"
            }}
          }
        }
      }
    }
  }
}


"ABYSSUS_HIVE_FLOOR_CANCER_TEST_BACKUP"
{
  name = "floor"
  viewId = { "abyssus_hive_floor_1" }
  luxury = 0.5
  layer = FLOOR
  canBuildOutsideOfTerritory = true
  tickType = Effect {
    Chance 0.03 {
      FirstSuccessful {
        Filter Or {FurnitureType "MOUNTAIN" FurnitureType "MOUNTAIN2" FurnitureType "SANDSTONE" FurnitureType "GLACIER" FurnitureType "HELL_MOUNTAIN"} {
          PlaceFurniture "ABYSSUS_HIVE_WALL"
        }
        Filter Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR" {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"
              }
            }
          }
        }
        Filter And {Translate {1 0} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {1 0} }
              }
            }
          }
        }
        Filter And {Translate {1 1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {1 1} }
              }
            }
          }
        }
        Filter And {Translate {1 -1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {1 -1} }
              }
            }
          }
        }
        Filter And {Translate {-1 0} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {-1 0} }
              }
            }
          }
        }
        Filter And {Translate {-1 1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {-1 1} }
              }
            }
          }
        }
        Filter And {Translate {-1 -1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {-1 -1} }
              }
            }
          }
        }
        Filter And {Translate {0 1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
          Filter FurnitureType "FLOOR" {
            Filter Or {Not FurnitureType "MOUNTAIN" Not FurnitureType "MOUNTAIN2" Not FurnitureType "SANDSTONE" Not FurnitureType "GLACIER" Not FurnitureType "HELL_MOUNTAIN"} {
              Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                CustomArea PlaceFurniture "ABYSSUS_HIVE_RANDOM_FLOOR"{ {0 1} }
              }
            }
          }
        }
        Filter And {Translate {0 -1} Not FurnitureType "ABYSSUS_HIVE_RANDOM_FLOOR"} {
