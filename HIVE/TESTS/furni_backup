"ABYSSUS_HIVE_FLOOR_CANCER_TEST"
{
  name = "floor"
  viewId = { "abyssus_hive_floor_1" }
  luxury = 0.5
  layer = FLOOR
  canBuildOutsideOfTerritory = true
  tickType = Effect {
    Chance 0.30 {
      Area 1 Filter FurnitureType "FLOOR" {
        Filter Not Area 1 FurnitureType "ABYSSUS_HIVE_FLOOR_CANCER_TEST" {
          Filter Not FurnitureType "MOUNTAIN" {
            Filter Not FurnitureType "MOUNTAIN2" {
              Filter Not FurnitureType "GLACIER" {
                Filter Not FurnitureType "SANDSTONE" {
                  Filter Not FurnitureType "HELL_MOUNTAIN" {
                    Filter Not FurnitureType "ABYSSUS_HIVE_FLOOR_CANCER_TEST" {
                      Filter Not FurnitureType "ABYSSUS_HIVE_WALL" {
                        Chance 0.10 PlaceFurniture "ABYSSUS_HIVE_FLOOR_CANCER_TEST"
                      }
                    }
                  }
                }
              }
            }
          }
          Filter FurnitureType "MOUNTAIN" {
            Chance 0.15 PlaceFurniture "ABYSSUS_HIVE_WALL"
          }
          Filter FurnitureType "MOUNTAIN2" {
            Chance 0.15 PlaceFurniture "ABYSSUS_HIVE_WALL"
          }
          Filter FurnitureType "SANDSTONE" {
            Chance 0.15 PlaceFurniture "ABYSSUS_HIVE_WALL"
          }
          Filter FurnitureType "GLACIER" {
            Chance 0.15 PlaceFurniture "ABYSSUS_HIVE_WALL"
          }
          Filter FurnitureType "HELL_MOUNTAIN" {
            Chance 0.15 PlaceFurniture "ABYSSUS_HIVE_WALL"
          }
        }
      }
    }
  }
}

